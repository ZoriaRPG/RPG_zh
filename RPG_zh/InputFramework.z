float BooleanToFloat(bool State)
{
	if(State == true)
	{
		return 1;
	}
	else
	{
		return 0;
	}
}

void QuickCircle(int x, int y, int radius)
{
	Screen->Circle(7, x, y, radius, 0x01, 1, 0, 0, 0, true, OP_TRANS);
}